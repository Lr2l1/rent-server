<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>rent-server</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>



	<servlet>
		<servlet-name>deleteUserAction</servlet-name>
		<servlet-class>boardServer/user/model/controller/DeleteFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteUserAction</servlet-name>
		<url-pattern>/deleteUserAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>joinFormAction</servlet-name>
		<servlet-class>boardServer/user/model/controller/JoinFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>joinFormAction</servlet-name>
		<url-pattern>/joinFormAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>loginFormAction</servlet-name>
		<servlet-class>boardServer/user/model/controller/LoginFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginFormAction</servlet-name>
		<url-pattern>/loginFormAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logoutAction</servlet-name>
		<servlet-class>boardServer/user/model/controller/LogoutAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoutAction</servlet-name>
		<url-pattern>/logoutAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>updateUserFormAction</servlet-name>
		<servlet-class>boardServer/user/model/controller/UpdateUserFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateUserFormAction</servlet-name>
		<url-pattern>/updateUserFormAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>findUserFormAction</servlet-name>
		<servlet-class>boardServer/user/model/controller/FindUserFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findUserFormAction</servlet-name>
		<url-pattern>/search/user</url-pattern>
	</servlet-mapping>
	
	
	<!-- JSP files mapping -->
	<servlet>
		<servlet-name>header</servlet-name>
		<jsp-file>/views/module/header.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>header</servlet-name>
		<url-pattern>/header</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>footer</servlet-name>
		<jsp-file>/views/module/footer.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>footer</servlet-name>
		<url-pattern>/footer</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>viewAll</servlet-name>
		<jsp-file>/views/car/viewAll.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>viewAll</servlet-name>
		<url-pattern>/viewAll</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>mainMyPage</servlet-name>
		<jsp-file>/views/myPage/mainMyPage.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>mainMyPage</servlet-name>
		<url-pattern>/mainMyPage</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/views/user/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>join</servlet-name>
		<jsp-file>/views/user/join.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>join</servlet-name>
		<url-pattern>/join</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateUser</servlet-name>
		<jsp-file>/views/user/updateUser.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateUser</servlet-name>
		<url-pattern>/updateUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteUser</servlet-name>
		<jsp-file>/views/user/deleteUser.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteUser</servlet-name>
		<url-pattern>/deleteUser</url-pattern>
	</servlet-mapping>


</web-app>